    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Namada Android",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/react-native/local-cli/cli.js",
            "args": [
              "run-android"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "sourceMaps": true
          },
        {
            "name": "Run Namada iOS",
            "type": "reactnative",
            "request": "launch",
            "platform": "ios",
            "sourceMaps": true
          }
    ]
}